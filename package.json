{
  "name": "city-energy-analyst-gui",
  "productName": "CityEnergyAnalyst-GUI",
  "version": "4.0.0-beta.3",
  "description": "Official GUI of the City Energy Analyst tool",
  "homepage": "http://www.cityenergyanalyst.com",
  "repository": "github:architecture-building-systems/CityEnergyAnalyst-GUI",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dev-electron": "vite --mode electron",
    "build-electron": "vite build --config vite.electron.config.js && vite build --mode electron",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "electron:dev": "concurrently \"yarn dev-electron\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "yarn build-electron && electron-builder build --publish never -c.mac.identity=null",
    "electron:release": "yarn build-electron && electron-builder build --publish always"
  },
  "keywords": [],
  "author": {
    "name": "Architecture and Building Systems",
    "email": "cea@arch.ethz.ch"
  },
  "maintainers": [
    {
      "name": "Reynold Mok",
      "email": "reynold.mok@arch.ethz.ch"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "electron": "^39.2.6",
    "electron-builder": "26.3.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.7.4",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@babel/polyfill": "^7.12.1",
    "@deck.gl-community/editable-layers": "9.1.1",
    "@fontsource/space-grotesk": "^5.2.10",
    "@mapbox/mapbox-gl-draw": "^1.5.1",
    "@mapbox/mapbox-gl-draw-static-mode": "^1.0.1",
    "@react-spring/web": "^10.0.3",
    "@tanstack/react-query": "^5.90.12",
    "@turf/turf": "^7.3.1",
    "antd": "^6.1.0",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "chartjs-plugin-dragdata": "^2.3.1",
    "color-interpolate": "^2.0.0",
    "date-fns": "^4.1.0",
    "deck.gl": "^9.2.5",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "html-react-parser": "^5.2.10",
    "javascript-color-gradient": "^2.5.0",
    "jszip": "^3.10.1",
    "maplibre-gl": "^5.14.0",
    "path-browserify": "^1.0.1",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "react-map-gl": "^8.1.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.10.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.8.1",
    "tabulator-tables": "^4.9.3",
    "zustand": "^5.0.9"
  },
  "main": "electron/main.mjs",
  "build": {
    "appId": "com.cityenergyanalyst.gui",
    "productName": "CEA-4 Desktop",
    "directories": {
      "output": "out",
      "buildResources": "build"
    },
    "extraMetadata": {
      "main": "dist-electron/main.js"
    },
    "files": [
      "dist/**",
      "dist-electron/**",
      "!node_modules/**/*"
    ],
    "mac": {
      "icon": "public/cea.png",
      "target": "default",
      "extraResources": [
        {
          "from": "dependencies",
          "to": "."
        }
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "icon": "public/cea.png",
      "target": [
        {
          "target": "nsis",
          "arch": "x64"
        }
      ],
      "publish": [
        "github"
      ]
    },
    "nsis": {
      "createDesktopShortcut": false,
      "createStartMenuShortcut": false
    },
    "generateUpdatesFilesForAllChannels": true
  },
  "packageManager": "yarn@4.12.0"
}
